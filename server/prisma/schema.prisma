generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Orders {
  id Int      @id @default(autoincrement())
  customerName   String
  addressDelivery String
  orderStatus  String

  @@map("orders")
}

model Users {
  id Int    @id @default(autoincrement())
  name  String
  email String @unique
  password String

  @@map("users")
}
